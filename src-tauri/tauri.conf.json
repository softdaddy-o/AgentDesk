{
    "$schema": "https://raw.githubusercontent.com/nicedoc/tauri2/main/tauri.conf.json",
    "productName": "AgentDesk",
    "version": "0.1.0",
    "identifier": "com.agentdesk.app",
    "build": {
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:1420",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist"
    },
    "app": {
        "windows": [
            {
                "title": "AgentDesk",
                "width": 1280,
                "height": 800,
                "minWidth": 800,
                "minHeight": 600,
                "decorations": true,
                "resizable": true
            }
        ],
        "security": {
            "csp": null
        }
    }
}
